<h1>Listing raw_data_scores</h1>

<% form_for :raw_data, :url => prepmassiveInsertion do |f| %>
  <p>
    Name: <%= f.text_field :name %>
  </p>
  <% for task in @project.tasks %>
    <% fields_for "project[task_attributes][]", task do |task_form| %>
      <p>
        Task: <%= task_form.text_field :name %>
      </p>
    <% end %>
  <% end %>
  <p><%= submit_tag "Create Project" %></p>
<% end %>



<table>
  <tr>
    <th>Scoredate</th>
    <th>Value</th>
    <th>Rawdata</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @scores.each do |raw_data_score| %>
  <tr>
    <td><%= raw_data_score.scoredate_id %></td>
    <td><%= raw_data_score.value %></td>
    <td><%= raw_data_score.rawdata.name %></td>
    <td><%= link_to 'Show', raw_data_score %></td>
    <td><%= link_to 'Edit', edit_raw_data_score_path(raw_data_score) %></td>
    <td><%= link_to 'Destroy', raw_data_score, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Raw data score', new_raw_data_score_path %>
